    function usarCupomDesconto(element, event) {
        var navigationPage = new String();
        var queries = window.location.href.split('&');
        for (i = 0; i < queries.length; i++) {
            if (queries[i] && $.trim(queries[i]).indexOf("cupom") < 0)
                navigationPage += $.trim(queries[i]);
