        function setCookie(cname, cvalue, exdays) {
            const d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            let expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }
        function getCookie(cname) {
            let name = cname + "=";
            let ca = document.cookie.split(';');
            for (let i = 0; i < ca.length; i++) {
                let c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        function getGuid() {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        }

        function checkCookie(name) {
            let cookievalue = getCookie(name);
            if (cookievalue == null || cookievalue.trim() == "") {
                setCookie(name, getGuid(), 730);
            }
        }
        let sessionEncerrado = false;
        $(function () {
            consultarSessionId();

            if (getCookie("idSessionECommerce") == "" || sessionEncerrado) {
                setCookie("idSessionECommerce", "")
            }
            checkCookie("idSessionECommerce");

            $('.botao_carrinho_pc a').attr("href", `${$('.botao_carrinho_pc a').attr("href")}&se=${getCookie("idSessionECommerce")}`);
            $('.botao_pedidos_pc a').attr("href", `${$('.botao_pedidos_pc a').attr("href")}&se=${getCookie("idSessionECommerce")}`);
            $('.botao_conta_pc a').attr("href", `${$('.botao_conta_pc a').attr("href")}&se=${getCookie("idSessionECommerce")}`);
        });

        function limparCookies() {
            Cookies.remove('idSessionECommerce');
        }

        function consultarSessionId() {
            let stringConsulta = "";
            if (window.location.href.toLowerCase().indexOf("localhost:") >= 0) {
                stringConsulta = "http://localhost:49297/api/ItensCarrinho.ashx";
            } else {
                stringConsulta = "https://carrinho.site/api/ItensCarrinho.ashx";
            }

            stringConsulta += `?guid=${getCookie("idSessionECommerce")}`;

            $.getJSON(stringConsulta, function (data) {
                if (!data.encerrado) {
                    $('.reg-itens').text(data.qtd_itens);
                    $('.reg-itens').show();
                    if (data.nome_cliente) {
                        $('.div_cadse').text(data.nome_cliente)
                    }
                } else {
                    /*limparCookies();*/
                    sessionEncerrado = true;
                }
            });
        }
    </script>

