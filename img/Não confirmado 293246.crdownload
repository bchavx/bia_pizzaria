    function getValorAtual(qe) {
        var idsSelecionados = idsCaracteristicasSelecionadas(true, ';');
        idsSelecionados = idsSelecionados.substring(0, idsSelecionados.lastIndexOf(';'));
        var valorRetorno = 0;
        $('#tecr div').each(function () {
            var idsEstoque = $(this).data('ids');
            if (verificarMesmoValores(idsSelecionados, idsEstoque)) {
                valorRetorno = parseFloat($(this).data('val').replace(',', '.'));
            }
        });

        var idsValoresSemEstoque = idsCaracteristicasSelecionadas(false, ' ').trim();

        idsValoresSemEstoque.split(' ').forEach(i => {
            let valSemEstoque = $('.select-caracteristica option[value="' + i + '"]').attr('valoradicional');
            if (valSemEstoque) {
                valorRetorno += valSemEstoque.replace(',', '.') * 1;
            }
        })


        if (typeof $('.preco_antigo').data('preco') != "undefined" && valorRetorno < $('.preco_antigo').data('preco').replace(",", ".") * 1) {
            $('.preco_antigo').show();
        } else {
            $('.preco_antigo').hide();
        }

        if (valorRetorno > 0) {
            $('.preco_atual').html("<span class='prefix'>R$</span> " + getValorCurrency(valorRetorno));
        } else if (valorRetorno <= 0) {
            $('.preco_atual').html("Consulte-nos");
        }
    }

    function reescreverDesconto() {
        if (typeof $(".line-through").text() == "undefined") {
            var valorPrincipal = $(".line-through").text().replace(".", "").replace(",", ".");
            var valorExibido = $(".preco_atual").text().toLowerCase().replace("r$", "").replace(".", "").replace(",", ".");
            var valorDesconto = (100 - (valorExibido * 100 / valorPrincipal)).toFixed(2);
            $(".preco-desconto").text(valorDesconto);
        }
    }

    function getEstoqueValoresSelecionados() {
        var idsSelecionados = idsCaracteristicasSelecionadas(true, ';');
        idsSelecionados = idsSelecionados.substring(0, idsSelecionados.lastIndexOf(';'));

        var valorRetorno = 0;
        $('#tecr div').each(function () {
            var idsEstoque = $(this).data('ids');
            if (verificarMesmoValores(idsSelecionados, idsEstoque)) {
                valorRetorno = parseFloat($(this).data('qe').replace(',', '.'));
            }
            valorRetorno = parseFloat(valorRetorno.toString().replace('.', ','));
        });
        return valorRetorno;
    }

    function verificarMesmoValores(ids1, ids2) {
        if (!ids1 || $.trim(ids1) == '' || !ids2 || $.trim(ids2) == '')
            return false;

        ids1 = ids1.toString();
        ids2 = ids2.toString();
        var valores1 = ids1.replace(' ', '').split(';');
        var valores2 = ids2.replace(' ', '').split(';');
        var mesmosValores = (valores1.length == valores2.length);
        if (mesmosValores)
            for (index = 0; index < valores1.length; index++) {
                if (ids2.indexOf($.trim(valores1[index])) < 0)
                    return false;
            }

        return mesmosValores;
    }

    function descricaoCaracteristicasSelecionadas() {
        if ($('.corpo_item_variacao_produto').find("select[data-influencia-estoque='true'] > option:selected").length < 1)
            return '';
        var retorno = ' - ';
        $('.corpo_item_variacao_produto').each(function (index) {
            if ($(this).find('.btn.active').length > 0) {
                var nomeCaracteristica = $.trim($(this).parent().find('.titulo_item_variacao_produto').text());
                var valorCaracteristica = $(this).find('.btn.active').find('.descricao_item_variacao').text();
                retorno += ((index > 0 ? '; ' : '') + (nomeCaracteristica + ':' + valorCaracteristica));
            }
        });
        return retorno;
    }

    function idsCaracteristicasSelecionadas(comEstoque, separator) {
        if ($("select[data-influencia-estoque='" + comEstoque + "']").find("option:selected").length < 1)
            return '';
        var retorno = '';
        $("select[data-influencia-estoque='" + comEstoque + "']").each(function (index) {
            if ($(this).find("option:selected").length > 0) {
                var idCaracteristica = $(this).find('option:selected').val();
                retorno += (idCaracteristica + separator);
            }
        });
        return retorno;
    }

