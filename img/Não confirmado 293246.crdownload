<script>
    function filtrarUrl(url) {
        var newGuid = Math.random().toString(36).replace(/[^a-z|1-9]+/g, '').substr(0, 5);
        var lista = url.substring(url.indexOf("?") + 1).split("&");
        var ancientGuid = "";
        for (var i = 0; i < lista.length; i++) {
            if (lista[i].split("=")[0] == "guid") {
                ancientGuid = lista[i].split("=")[1];
                break;
            }
        }
        if (ancientGuid != "")
            return url.replace(ancientGuid, newGuid);
        else
            return url;

    }

