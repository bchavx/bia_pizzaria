    function getValorCurrency(valor) {
        valor = valor.toFixed(2);
        let preComma = valor.split(".")[0];
        let posComma = valor.split(".")[1];

        let retorno = "";
        let countComma = 1;
        for (var i = preComma.length - 1; i >= 0; i--) {
            retorno = preComma[i] + retorno;
            if (countComma % 3 == 0 && i > 0) {
                retorno = "." + retorno;
            }
            countComma++;
        }

        return retorno + "," + posComma;
    }

    // MÃ©todo principal para reorganizar os estoques
    function reorganizarListaDeEstoque() {
        var listIds = [];

        $(".item_variacao_produto .select-caracteristica[data-influencia-estoque='true']").each(function () {
            listIds.push(getOpcoesComoLista($(this)));
        });

        redesenharTECR(combos(listIds));
    }

    // Gera uma matriz de lista de caracteristicas sendo que cada caracteristica tem uma lista de valores
    function getOpcoesComoLista(elemento) {
        var lista = [];
        elemento.find("option").each(function () {
            lista.push($(this).attr("value"));
        });
        return lista;
    }

    // Faz a permuta das caracteristicas
    function combos(list, n = 0, result = [], current = []) {
        if (n === list.length) result.push(current)
        else list[n].forEach(item => combos(list, n + 1, result, [...current, item]))

        return result
    }
    // ResponsÃ¡vel por desenhar a nova ordem dos estoques
    function redesenharTECR(array) {
        var _localestoques = "";
        for (var i = 0; i < array.length; i++) {
            $("#tecr > div").each(function () {
                var cont = 0;
                for (var j = 0; j < array[i].length; j++) {
                    if ($(this).attr("data-ids").indexOf(array[i][j].toString()) > -1) {
                        cont++;
                    }
                    if (cont == array[i].length) {
                        _localestoques += "<div data-ids='" + $(this).attr("data-ids") + "' data-qe='" + $(this).attr("data-qe") + "' data-val='" + $(this).attr("data-val") + "'></div>";
                    }
                }
            });
        }
        $("#tecr").html(_localestoques);
    }

    // essa funÃ§Ã£o faz a seleÃ§Ã£o da primeira combinaÃ§Ã£o de caracteristica com o estoque diferente de 0
    function refreshSelectInicial() {
        if (window.location.href.toLowerCase().indexOf("cval=") == -1) {
            var estoque = null;
            $("#tecr div").each(function () {
                if ($(this).data("qe").replace(",", ".") * 1 > 0 && estoque == null) {
                    estoque = $(this).data("ids") + "";
                }
            });
            if (estoque != null && estoque.length > 0 && $(".descricao_item_variacao").text().toLowerCase() != "Ãºnico") {
                estoque = estoque.split(";")

                for (var i = 0; i < estoque.length; i++) {
                    $(".select-caracteristica[data-influencia-estoque='true'] option").each(function () {
                        if (typeof estoque[i] != "undefined" && $(this).val() == estoque[i].trim()) {
                            $(this).parent().val(estoque[i].trim());
                        }
                    });
                }

            }
        } else {
            var url = decodeURI(window.location.search).replace("?", "").split("&");
            for (var i = 0; i < url.length; i++) {
                if (url[i].split("=")[0] == "cval") {
                    url = url[i].split("=")[1].split(";");
                }
            }

            for (var i = 0; i < url.length; i++) {
                $(".select-caracteristica option").each(function () {
                    if (typeof url[i] != "undefined" && $(this).val() == url[i].split("_")[1]) {
                        $(this).parent().val(url[i].split("_")[1]);
                    }
                });
            }
        }
    }

    function setThisSelected(elemento) {
        elemento.click();
    }

    function carregarIdsCarrinhoProprio() {
        var idsValoresComEstoque = idsCaracteristicasSelecionadas(true, ';');
        var idsValoresSemEstoque = idsCaracteristicasSelecionadas(false, ';');
        var urlAtual = $('#carrinho_proprio').find('a').attr('href');
        var personalizacaoExtra = "";

        if (urlAtual != undefined && urlAtual.indexOf('contato.aspx') >= 0) {
            $('.preco_atual').html("Consulte-nos");
            return;
        }


        if ($("#ctl00_ContentPlaceHolder1_ControlProduto_pnlPersonalizacao").length > 0 && $("#ctl00_ContentPlaceHolder1_ControlProduto_txtConteudoPersonalizacao").val().length > 0) {
            personalizacaoExtra += "&ext=" + $("#ctl00_ContentPlaceHolder1_ControlProduto_txtConteudoPersonalizacao").val();
        }

        if ((typeof $('#carrinho_proprio').find('a').attr('href')) == "undefined") {
            var parametro = $('#carrinho_proprio').attr("action");
            parametro = (parametro != undefined ? parametro.substring(0, parametro.indexOf("vc=")) : parametro);
            $('#carrinho_proprio').attr("action", parametro + "vc=" + idsValoresComEstoque + "&vcnot=" + idsValoresSemEstoque + personalizacaoExtra);

