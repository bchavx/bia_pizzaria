                    function carregarMaisComentarios() {
                        if ($('.lista_comentarios .item_comentario').length > 5) {
                            $('.btn-ver-mais').show();
                        }
                        let countDisplayedItem = 0;
                        $('.lista_comentarios .item_comentario').each(function () {
                            if ($(this).css('display') == 'none') {
                                countDisplayedItem++;
                                $(this).css('display', 'inherit');
                            }
                            if (countDisplayedItem == 5) { return false; }
                        });
                        if (countDisplayedItem < 5) {
                            $('.btn-ver-mais').hide();
                        }
